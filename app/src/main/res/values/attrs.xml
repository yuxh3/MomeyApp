<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="head_text_style">
        <attr name="ivsiblility" format="enum">
            <enum name="visiblie" value="0"/>
            <enum name="invisible" value="1"/>
            <enum name="gone" value="2"/>
        </attr>

        <attr name="center" format="string"/>
        <attr name="leftIcon" format="reference"/>
        <attr name="leftText" format="string"/>
        <attr name="right" format="reference"/>

    </declare-styleable>

    <declare-styleable name="RVPIndicato">
        <attr name="item_count" format="integer"/>
        <attr name="text_color_normal" format="color"/>
        <attr name="text_color" format="color"/>
        <attr name="text_color_hightlight" format="color"/>
        <attr name="text_size" format="dimension"/>
        <attr name="indicator_color" format="color"/>
        <attr name="indicator_src" format="reference"/>
        <attr name="indicator_style" format="enum">
            <enum name="bitmap" value="0" />
            <enum name="line" value="1" />
            <enum name="square" value="2" />
            <enum name="triangle" value="3" />
        </attr>

    </declare-styleable>

    <declare-styleable name="RoundProgressBar">
        <attr name="roundColor" format="color"/>
        <attr name="roundProgressColor" format="color"/>
        <attr name="textColor" format="color"/>
        <attr name="textSize" format="dimension"/>
        <attr name="roundWidth" format="integer"/>
        <attr name="text" format="string"/>
        <attr name="max" format="integer"/>
        <attr name="textIsDisplayable" format="boolean"/>
        <attr name="style" format="integer"/>
    </declare-styleable>

</resources>